{
  "extends": "@react-native/typescript-config/tsconfig.json",
  "compilerOptions": {
    "jsx": "react-native",
    "types": ["jest", "node"],
    "baseUrl": "./src",
    "paths": {
      "@src/*": ["./*"],
      "@assets/*": ["./assets/*"],
      "@components/*": ["./components/*"],
      "@fixtures/*": ["./fixtures/*"],
      "@hooks/*": ["./hooks/*"],
      "@navigation/*": ["./navigation/*"],
      "@reusable/*": ["./components/reusable/*"],
      "@screens/*": ["./screens/*"],
      "@services/*": ["./services/*"],
      "@store/*": ["./store/*"],
      "@appTypes/*": ["./types/*"],
      "@utils/*": ["./utils/*"],
    }
  },
  "include": ["**/*.ts", "**/*.tsx", "**/*.js", "**/*.jsx", "**/*.d.ts"],
}
